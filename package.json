  "main": "index.js",
  "scripts": {
    "hygen": "hygen",
    "codegen": "graphql-codegen",
    "generate": "graphql-codegen --config codegen.yml",
    "start": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "cd frontend && npm run dev",
